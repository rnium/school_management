<odoo>
    <record id="school_management_hogwarts" model="school_management.school">
        <field name="name">Hogwarts School of Witchcraft and Wizardry</field>
        <field name="address">Scotland, United Kingdom</field>
        <field name="contact">+44 1234 567890</field>
        <field name="email">saiful.islam@bjitacademy.com</field>
