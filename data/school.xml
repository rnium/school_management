                    ref('teacher_h_11'),
                    ref('teacher_h_12'),
                    ref('teacher_h_13'),
                    ref('teacher_h_14'),
                ])]"
        />
        <field
                name="student_ids"
                eval="[(6, 0, [
                    ref('student_h_1'),
                    ref('student_h_2'),
                    ref('student_h_3'),
                    ref('student_h_4'),
                    ref('student_h_5'),
                    ref('student_h_6'),
                    ref('student_h_7'),
                    ref('student_h_8'),
                    ref('student_h_9'),
                    ref('student_h_10'),
                    ref('student_h_11'),
                    ref('student_h_12'),
                    ref('student_h_13'),
                    ref('student_h_14'),
                    ref('student_h_15'),
                    ref('student_h_16'),
                    ref('student_h_17'),
                    ref('student_h_18'),
                    ref('student_h_19'),
                    ref('student_h_20'),
                ])]"
        />
        <field
                name="playground_ids"
                eval="[(6, 0, [
                    ref('playground_h_1'),
                    ref('playground_h_2'),
                    ref('playground_h_3'),
                    ref('playground_h_4'),
                    ref('playground_h_5'),
                ])]"
        />
    </record>

